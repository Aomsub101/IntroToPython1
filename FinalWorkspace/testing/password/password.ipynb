    "import base64\n",
    "plain_text = \"Hello, this is Mark\"\n",
    "\n",
    "encode = base64.b64encode(plain_text.encode('utf-8'))\n",
    "\n",
    "print(\"Encode: \", encode)\n",
    "\n",
    "decode = base64.b64decode(encode).decode('utf-8')\n",
    "\n",
    "print(\"Decode: \", decode)"
